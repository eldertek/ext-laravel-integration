{
  "name": "plesk/ext-laravel-integration",
  "description": "Plesk Laravel Toolkit integration with Laravel applications.",
  "type": "library",
  "license": "Apache-2.0",
  "keywords": [
    "Laravel",
    "Plesk"
  ],
  "require": {
    "php": ">=8.2",
    "laravel/framework": "^7.0 || ^10.0 || ^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "PleskExtLaravel\\": "src/"
    }
  },
  "extra": {
    "component": "package",
    "laravel": {
      "providers": [
        "PleskExtLaravel\\Providers\\ConsoleServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
